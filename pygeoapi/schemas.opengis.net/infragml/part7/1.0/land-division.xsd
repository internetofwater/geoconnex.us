	<!--                                                                                                      -->
	<!--  BEExtrudedFace                                                                                        -->
	<!--                                                                                                      -->
	<element name="BEExtrudedFace" type="lild:BEExtrudedFaceType" substitutionGroup="lild:BEFace"/>
