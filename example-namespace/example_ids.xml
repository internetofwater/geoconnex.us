<?xml version="1.0" encoding="UTF-8"?>
<backup xmlns="urn:csiro:xmlns:pidsvc:backup:1.0">
  <mapping>
    <path>/example</path>
    <type>1:1</type>
    <description>This is an example PID with two conditions</description>
    <creator>dblodgett@usgs.gov</creator>
    <action>
      <type>303</type>
      <name>location</name>
      <value>https://geoconnex.internetofwater.dev</value>
    </action>
    <conditions>
      <condition>
        <type>QueryString</type>
        <match>f?=.*</match>
        <actions>
          <action>
            <type>303</type>
            <name>location</name>
            <value>https://geoconnex.internetofwater.dev?f=${C:f:1}</value>
          </action>
        </actions>
      </condition>
      <condition>
        <type>Extension</type>
        <match>^.html$</match>
        <actions>
          <action>
            <type>303</type>
            <name>location</name>
            <value>https://geoconnex.internetofwater.dev.html</value>
          </action>
        </actions>
      </condition>
    </conditions>
  </mapping>
</backup>
